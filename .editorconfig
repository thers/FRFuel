
[*]

# Microsoft .NET properties
csharp_indent_braces = false
csharp_new_line_before_open_brace = accessors,anonymous_methods,control_blocks,events,indexers,lambdas,local_functions,methods,properties,types
csharp_preserve_single_line_blocks = true
csharp_space_after_cast = false

# ReSharper properties
resharper_blank_lines_after_block_statements = 0
resharper_blank_lines_after_start_comment = 0
resharper_blank_lines_around_single_line_property = 1
resharper_blank_lines_around_single_line_type = 0
resharper_blank_lines_before_single_line_comment = 1
resharper_csharp_blank_lines_inside_region = 0
resharper_csharp_case_block_braces = end_of_line
resharper_csharp_empty_block_style = together_same_line
resharper_csharp_insert_final_newline = true
resharper_csharp_keep_blank_lines_in_code = 100
resharper_csharp_max_line_length = 240
resharper_indent_pars = outside
resharper_keep_existing_embedded_block_arrangement = false
resharper_keep_existing_enum_arrangement = false
resharper_place_expr_accessor_on_single_line = true
resharper_place_method_attribute_on_same_line = true
resharper_place_type_attribute_on_same_line = true
resharper_remove_blank_lines_near_braces_in_code = false
resharper_remove_blank_lines_near_braces_in_declarations = false
